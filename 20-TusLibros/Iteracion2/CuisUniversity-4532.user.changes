!TransferNet methodsFor: 'as yet unclassified' stamp: 'ds 10/24/2021 11:57:48'!
summary

	| collection |
	collection := OrderedCollection new.
	
	account transactions do: [:transaction |
		(transaction isKindOf: Deposit) ifTrue: [collection addLast: 'Depsito por ' , transaction value asString , '.'].
		(transaction isKindOf: Withdraw) ifTrue: [collection addLast: 'Extraccin por ' , transaction value asString , '.'].
		(transaction isKindOf: WithdrawLeg) ifTrue: [collection addLast: 'Salida por transferencia de ' , transaction value asString , '.'].
		(transaction isKindOf: DepositLeg) ifTrue: [collection addLast: 'Entrada por transferencia de ' , transaction value asString , '.'].
	].

	collection addLast: 'Balance = ', (account balance asString).
	
	^ collection.
	
	! !

