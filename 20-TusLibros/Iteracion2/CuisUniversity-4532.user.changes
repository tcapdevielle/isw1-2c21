	|results preservedItems |
	
	self ensureValidQuery: aPrefix.
	
	results := OrderedCollection new.

	preservedItems := OOStack new.
	
	aStack size timesRepeat: [
		| elem |
		elem := aStack pop.
		"(self isPrefix: aPrefix ofString: elem) ifTrue: [results addLast: elem]."
		(elem beginsWith: aPrefix) ifTrue: [results addLast: elem].
		preservedItems push: elem.	
	].

	preservedItems size timesRepeat: [
		| elem |
		elem := preservedItems pop.
		aStack push: elem.
	].
	
	^ results.! !

