!ReceptiveAccount methodsFor: 'accounts' stamp: 'ds 10/16/2021 17:57:54' overrides: 50936717!
assertCanBeAdded: anAccount
	
	anAccount = self ifTrue: [self error: Portfolio accountAlreadyExistsError].
	^ true.
! !
!Portfolio methodsFor: 'accounts' stamp: 'ds 10/16/2021 17:57:54' overrides: 50936717!
assertCanBeAdded: anAccount 
	" for each children of the given account: assert that it is not included in this portfolio's descendants "
	| hasNoRepetitions |
	hasNoRepetitions := anAccount allChildren allSatisfy: [:foreignChild | (self allChildren includes: foreignChild) not].
	hasNoRepetitions ifFalse: [self error: Portfolio accountAlreadyExistsError].
	^ true.! !
